                        <ph props="autonumber" class="- topic/ph ">(1)</ph> 
                        <ph rev="Change 180  May 2, 2024" class="- topic/ph ">FAR <xref scope="external"
                                 format="html"
                                 href="https://www.acquisition.gov/far/52.219-11"
                                 class="- topic/xref ">52.219-11</xref>, Special 8(a) Contract Conditions; </ph>
