							              <colspec colnum="2"
                              colname="2"
                              colwidth="50*"
                              class="- topic/colspec "
                              rowheader="headers"/>
							              <thead class="- topic/thead ">
								                <row rowsep="1" class="- topic/row ">
									                  <entry colname="1" class="- topic/entry ">
										                    <p outputclass="CBottom" class="- topic/p " audience="GSAR">Subjective Characteristics</p>
									                  </entry>
									                  <entry colname="2" class="- topic/entry ">
										                    <p outputclass="CBottom" class="- topic/p " audience="GSAR">Objective Characteristics</p>
									                  </entry>
								                </row>
							              </thead>
							              <tbody class="- topic/tbody ">
								                <row rowsep="1" class="- topic/row ">
									                  <entry colname="1" class="- topic/entry ">
										                    <p class="- topic/p " audience="GSAR"/>
									                  </entry>
									                  <entry colname="2" class="- topic/entry ">
